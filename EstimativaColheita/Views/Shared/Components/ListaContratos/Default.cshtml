<div class="dropdown">
  <a class="btn btn-secondary dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-bs-toggle="dropdown" aria-expanded="false">
    Contratos
  </a>

  <ul class="dropdown-menu" aria-labelledby="dropdownMenuLink">
        @foreach(var categoria in Model)
        {
            <li><a class="dropdown-item" asp-controller="Talhao"
                 asp-action="ListContratos" style="color:black"
                 asp-route-categoria="">@categoria.DescricaoCompleta</a>
            </li>                 
        }
        <li class="dropdown-divider"></li>
                <li><a class="dropdown-item" asp-controller="Lanche"
                 asp-action="List" style="color:black"
                 asp-route-categoria="">Todos os lanches</a>
            </li>
  </ul>
</div>